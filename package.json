{
  "name": "react-select-datepicker",
  "version": "3.0.0",
  "description": "A simple and reusable dropdown datepicker component for React",
  "author": "JMcAmmond",
  "license": "MIT",
  "repository": "JMcAmmond/react-select-datepicker",
  "bugs": {
    "url": "https://github.com/JMcAmmond/react-select-datepicker/issues"
  },
  "homepage": "https://github.com/JMcAmmond/react-select-datepicker#readme",
  "main": "dist/index.js",
  "source": "src/index.tsx",
  "keywords": [
    "react",
    "datepicker",
    "date",
    "select",
    "reactjs",
    "dropdown",
    "datepicker-component"
  ],
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": ">=17.0.2 <20",
    "react-dom": ">=17.0.2 <20"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-url": "^6.1.0",
    "@storybook/addon-a11y": "^10.1.11",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-links": "^10.1.11",
    "@storybook/manager-api": "^8.6.14",
    "@storybook/react": "^10.1.11",
    "@storybook/react-vite": "^10.1.11",
    "@svgr/rollup": "^6.2.1",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^16.11.22",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/ui": "^4.0.17",
    "babel-loader": "^8.2.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-json-files": "^1.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-progress": "0.0.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "gh-pages": "^6.3.0",
    "jsdom": "^27.4.0",
    "postcss": "^8.4.7",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.68.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "storybook": "^10.1.11",
    "styled-components": "^5.3.5",
    "tslib": "^2.3.1",
    "typescript": "^4.9.5",
    "url-loader": "^4.1.1",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  },
  "scripts": {
    "test": "npm run test:eslint && npm run test:conflicts && npm run test:ts && npm run test:unit",
    "test:eslint": "npx eslint --config eslint.config.js \"src/**/*.{js,jsx,ts,tsx}\"",
    "test:conflicts": "eslint-config-prettier .eslint.config.js",
    "test:ts": "npx tsc -p ./tsconfig.json --noEmit",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "start": "storybook dev -p 6006",
    "bundle": "rollup -c",
    "clean": "rm -rf ./dist ./storybook-static",
    "build": "npm run clean && storybook build && npm run bundle",
    "deploy": "gh-pages -d storybook-static"
  },
  "eslintConfig": {
    "extends": [
      "./.eslintrc.cjs"
    ],
    "parserOptions": {
      "project": [
        "./tsconfig.json"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
